    "@koa/router": "^12.0.0",
    "@prisma/client": "^4.4.0",
    "discord.js": "^14.4.0",
    "discordx": "^11.3.0",
    "koa": "^2.13.4",
    "koa-body": "4.2.0",
    "node-fetch": "^3.2.10",
    "prisma": "^4.4.0"
  },
  "devDependencies": {
    "@types/koa": "^2.13.5",
    "@types/koa-bodyparser": "4.3.7",
    "@types/koa__router": "^12.0.0",
    "@types/node": "^18.7.18",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "4.8.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  }
